#include <iostream>
#include <vector>

using namespace std;

int main()
{
    int n, m;
    cout << "Введите число с которым сравниваем ";
    cin >> n;
    for (int i = 0; i < n; ++i) {
        cout << "Введите число";
        string s; int sum = 0;
        cin >> m;
        if (m == 1) {
            cout << -1;
        }
        else {
            cout << 2;
            for (int j = 0; j < m - 1; ++j)
                cout << 3;
        }
        cout << endl;
    }
    return 0;
}
